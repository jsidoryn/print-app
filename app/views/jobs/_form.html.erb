<div class="inputs">
  <%- unless f.error_notification.blank? -%>
    <%= f.error_notification %>
  <%- end -%>
  <%= f.input :title %>
  <%= f.input :job_number %>
  <%= f.input :description %>
  <%= f.input :organisations, collection: Organisation.clients %>
</div>
<div class="actions">
  <%= f.button :button, "Submit", class: "button" -%> <%= link_to "Cancel", jobs_path %>
</div>
